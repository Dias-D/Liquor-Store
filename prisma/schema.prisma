// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id             String @id
  Nome           String
  DataNascimento String

  @@map("clientes")
}

model Catalogo {
  id            String @id
  Marca         String
  Classificacao String
  Nome          String
  TeorAlcoolico String
  Regiao        String
  PrecoAtual    Float

  @@map("catalogos")
}
